#: 11080000.xhp
msgctxt ""
"11080000.xhp\n"
"par_id3145673\n"
"help.text"
msgid "SELECT \"Address\" FROM \"biblio\" \"biblio\""
msgstr "SELECT \"Adresse\" FROM \"biblio\" \"biblio\""

