msgctxt ""
"dabawiz02jdbc.xhp\n"
"par_idN1068C\n"
"help.text"
msgid "mysql://hostname:port/database_name"
msgstr "mysql://nomamfitrió:port/nom_basededades"

#. m5Kcx
#: dabawiz02jdbc.xhp
msgctxt ""
"dabawiz02jdbc.xhp\n"
"par_idN10695\n"
"help.text"
