msgctxt ""
"rep_sort.xhp\n"
"par_id5833307\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Select to show or hide the Group Header.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Seleccioneu si voleu que es mostri o s'amagui la Capçalera de grup.</ahelp>"

#. tN76n
#: rep_sort.xhp
msgctxt ""
"rep_sort.xhp\n"
"par_id7726676\n"
"help.text"
