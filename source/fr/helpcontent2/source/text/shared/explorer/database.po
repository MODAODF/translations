"par_id3147243\n"
"help.text"
msgid "<ahelp hid=\"dbaccess/ui/password/newpassword\">Enter the new password.</ahelp>"
msgstr "<ahelp hid=\"dbaccess/ui/password/newpassword\">Saisissez le nouveau mot de passe.</ahelp>"

