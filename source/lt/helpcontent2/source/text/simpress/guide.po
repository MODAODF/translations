"help.text"
msgid "<bookmark_value>copying; slides</bookmark_value><bookmark_value>slides; copying between documents</bookmark_value><bookmark_value>pages; copying</bookmark_value><bookmark_value>inserting; slides from files</bookmark_value><bookmark_value>pasting;slides from other presentations</bookmark_value>"
msgstr "<bookmark_value>kopijavimas; skaidrės</bookmark_value><bookmark_value>skaidrės; kopijavimas iš vieno dokumento į kitą</bookmark_value><bookmark_value>puslapiai; kopijavimas</bookmark_value><bookmark_value>įterpimas; skaidrės iš failų</bookmark_value><bookmark_value>įdėjimas;skaidrės iš kitų pateikčių</bookmark_value>"

