"help.text"
msgid "<bookmark_value>kiosk export</bookmark_value><bookmark_value>HTML; live presentations</bookmark_value><bookmark_value>live presentations on the Internet</bookmark_value><bookmark_value>showing;live presentations on the Internet</bookmark_value><bookmark_value>presentations; live on the Internet</bookmark_value><bookmark_value>Internet; presentations</bookmark_value><bookmark_value>WebCast export</bookmark_value>"
msgstr "<bookmark_value>eksportas automatiniam demonstravimui</bookmark_value><bookmark_value>HTML; pateikties demonstravimas internetu</bookmark_value><bookmark_value>pateikties demonstravimas internetu</bookmark_value><bookmark_value>rodymas;pateikties demonstravimas internetu</bookmark_value><bookmark_value>pateiktys; demonstravimas internetu</bookmark_value><bookmark_value>internetas; pateiktys</bookmark_value><bookmark_value>WebCast eksportas</bookmark_value>"

